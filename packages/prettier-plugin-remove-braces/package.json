        "prepublishOnly": "npm run build",
        "test": "node --test"
    },
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.com/"
    },
    "devDependencies": {
        "@rslib/core": "^0.15.0",
        "@types/node": "^24.7.2",
        "typescript": "^5.8.3"
    },
    "peerDependencies": {
        "prettier": "^3.7.3"
    }
