{
    "name": "@1adybug/prettier-plugin-sort-imports",
    "version": "0.0.30",
    "description": "一个 Prettier 插件，用于对 JavaScript/TypeScript 文件的导入语句进行分组和排序",
    "keywords": [
        "prettier",
        "plugin",
        "import",
        "sort",
        "organize",
        "typescript",
        "javascript"
    ],
    "author": "1adybug <lurongv@qq.com>",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/1adybug/prettier.git"
    },
    "homepage": "https://github.com/1adybug/prettier/tree/main/packages/prettier-plugin-sort-imports",
    "bugs": {
        "url": "https://github.com/1adybug/prettier/issues"
    },
    "type": "module",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "require": "./dist/index.js"
        }
    },
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "files": [
        "dist"
    ],
    "sideEffects": false,
    "scripts": {
        "build": "rslib build",
        "dev": "rslib build --watch",
        "prepublishOnly": "npm run build",
        "test": "bun test",
        "test:watch": "bun test --watch"
    },
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.com/"
    },
    "dependencies": {
        "@babel/core": "^7.28.6",
        "@babel/parser": "^7.28.6",
        "@babel/traverse": "^7.28.6",
        "@babel/types": "^7.28.6"
    },
    "devDependencies": {
        "@rslib/core": "^0.15.1",
        "@types/babel__core": "^7.20.5",
        "@types/babel__traverse": "^7.28.0",
        "@types/bun": "latest",
        "@types/node": "^24.10.8",
        "json5": "^2.2.3",
        "supports-color": "^10.2.2",
        "typescript": "^5.9.3"
    },
    "peerDependencies": {
        "prettier": "^3.8.0"
    }
}
