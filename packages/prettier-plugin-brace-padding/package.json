{
    "name": "@1adybug/prettier-plugin-block-padding",
    "version": "0.0.13",
    "description": "A Prettier plugin that intelligently adds blank lines around code blocks for better visual grouping",
    "keywords": [
        "prettier",
        "prettier-plugin",
        "formatter",
        "code-formatting",
        "blank-lines",
        "spacing",
        "block-padding",
        "typescript",
        "javascript"
    ],
    "author": "1adybug <lurongv@qq.com>",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/1adybug/prettier.git"
    },
    "homepage": "https://github.com/1adybug/prettier/tree/main/packages/prettier-plugin-brace-padding",
    "bugs": {
        "url": "https://github.com/1adybug/prettier/issues"
    },
    "type": "module",
    "exports": {
        ".": {
