{
    "name": "@1adybug/prettier",
    "version": "0.0.11",
    "description": "推荐的 Prettier 配置",
    "keywords": [
        "prettier",
        "plugin"
    ],
    "author": "1adybug <lurongv@qq.com>",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/1adybug/prettier.git"
    },
    "homepage": "https://github.com/1adybug/prettier/tree/main/packages/prettier",
    "bugs": {
        "url": "https://github.com/1adybug/prettier/issues"
    },
    "type": "module",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "require": "./dist/index.js"
        }
    },
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "files": [
        "dist"
    ],
    "sideEffects": false,
    "scripts": {
        "build": "rslib build",
        "dev": "rslib build --watch",
        "prepublishOnly": "npm run build"
    },
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.com/"
    },
    "dependencies": {
        "@1adybug/prettier-plugin-block-padding": "workspace:*",
        "@1adybug/prettier-plugin-remove-braces": "workspace:*",
        "@1adybug/prettier-plugin-sort-imports": "workspace:*",
        "prettier-plugin-tailwindcss": "^0.7.1",
        "tsconfig-paths": "^4.2.0"
    },
    "devDependencies": {
        "@rslib/core": "^0.18.2",
        "@types/node": "^24.10.1",
        "typescript": "^5.9.3"
    },
    "peerDependencies": {
        "prettier": "^3.0.0"
    }
}
